"VertexLitGeneric"
{
	"$baseTexture" "models/HammerheadSwirl"
	$additive 1
	$selfillum 1
	"$nocull" "1"
	"$model" "1"
	
	"$emissiveBlendEnabled"     "1" // Enables effect
	"$emissiveBlendTexture"     "models/HammerheadSwirl"
	"$emissiveBlendBaseTexture" "models/HammerheadSwirl"
	"$emissiveBlendFlowTexture" "models/HammerheadSwirl_flowmap"
	"$emissiveBlendTint"        "[5 5 5]"
	"$emissiveBlendScrollVector" "[0.01 0.01]"
	"$emissiveBlendStrength" 		"1"
	
	"$time" "0.0"
	"Proxies"
	{
		"TextureScroll"
		{
			"textureScrollVar" "$basetexturetransform"
			"textureScrollRate" 0.1
			"textureScrollAngle" -100
		}
		Sine // a proxy which produces a sine wave
		{
			resultVar	$color2 // The shader parameter to be manipulated
			sineperiod	5
			sinemin		1
			sinemax		3
		}
	}
}
