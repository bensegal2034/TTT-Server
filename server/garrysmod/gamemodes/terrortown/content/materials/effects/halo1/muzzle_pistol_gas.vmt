"UnlitGeneric"
{
	"$basetexture"	"effects/halo1/muzzle_pistol_gas"
	"$additive"	1
	"$vertexcolor"	1
	"$vertexalpha"	1
	"$translucent" 				"1"

"Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "30"
      }
   }

}